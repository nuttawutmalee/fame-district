{
  "name": "wink-hotels",
  "version": "1.0.0",
  "description": "Website built from Prismic, Gatsby, and React.",
  "main": "n/a",
  "scripts": {
    "test": "jest",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*.js'",
    "lint": "eslint ./src/**/*.js",
    "create-env": "rimraf .env && ncp .env.example .env",
    "start": "gatsby develop",
    "build": "gatsby build",
    "create-stack": "export $(cat .env | xargs) && ./bin/create.sh",
    "update-stack": "export $(cat .env | xargs) && ./bin/update.sh",
    "delete-stack": "export $(cat .env | xargs) && ./bin/delete.sh",
    "precommit": "npm run test",
    "postinstall": "npm run create-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuttawutmalee/wink-hotels.git"
  },
  "author": "Nuttawut Malee <nuttawutmalee@gmail.com>",
  "license": "ISC",
  "keywords": [
    "website",
    "gatsby",
    "prismic",
    "react",
    "styled-components"
  ],
  "bugs": {
    "url": "https://github.com/nuttawutmalee/wink-hotels/issues"
  },
  "jest": {
    "verbose": false
  },
  "homepage": "https://github.com/nuttawutmalee/wink-hotels#readme",
  "dependencies": {
    "aws-amplify": "^1.1.19",
    "aws-sdk": "^2.392.0",
    "axios": "^0.18.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "gatsby": "^2.0.55",
    "gatsby-cli": "^2.4.5",
    "gatsby-image": "^2.0.20",
    "gatsby-link": "^2.0.6",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-react-svg": "^2.0.0",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^2.0.13",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-source-prismic": "^2.1.0",
    "gatsby-transformer-sharp": "^2.1.8",
    "glslify-loader": "^1.0.2",
    "intl": "^1.2.5",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "ptz-i18n": "^0.4.0",
    "react": "^16.6.3",
    "react-device-detect": "^1.6.1",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "react-loader-spinner": "^2.2.0",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "serverless": "^1.36.2",
    "styled-components": "^4.1.1",
    "three": "^0.98.0"
  },
  "devDependencies": {
    "aws-sdk-mock": "^4.3.0",
